00290 {\cf18 void} cppMD01Frame::OnCellClick(wxGridEvent& event)\par
00291 \{\par
00292     {\cf20 // Get the grid from the event}\par
00293     wxGrid* grid = {\cf17 static_cast<}wxGrid*{\cf17 >}({\cf17 event}.GetEventObject());\par
00294 \par
00295     {\cf20 // Get the clicked cell's row and column}\par
00296     row = {\cf17 event}.GetRow();\par
00297     {\cf18 int} col = {\cf17 event}.GetCol();\par
00298 \par
00299     {\cf20 // Retrieve the cell value as a wxString}\par
00300     wxString cellValue = grid->GetCellValue(row, col);\par
00301 \par
00302     {\cf20 // Check if the cell value starts with "http"}\par
00303     {\cf19 if} (cellValue.StartsWith({\cf22 "http"}))\par
00304     \{\par
00305         {\cf20 // Open the URL in the default web browser}\par
00306         wxLaunchDefaultBrowser(cellValue);\par
00307     \}\par
00308     {\cf19 else} {\cf19 if} (cellValue.find({\cf22 ".pdf"}) != wxNOT_FOUND)\par
00309     \{\par
00310         cmd = {\cf22 "okular \\""};\par
00311         std::string tbuf;\par
00312         tbuf = cellValue.ToStdString();\par
00313         cmd = cmd + tbuf + {\cf22 "\\""};\par
00314         system(cmd.c_str());\par
00315     \}\par
00316     {\cf19 else} {\cf19 if} (cellValue.find({\cf22 ".odt"})  != wxNOT_FOUND)\par
00317     \{\par
00318         std::string pbuf =  {\cf22 "\\""} + cellValue.ToStdString() + {\cf22 "\\" & "};\par
00319         cmd = {\cf22 "openoffice4 -writer "} + pbuf;\par
00320         system(cmd.c_str());\par
00321     \}\par
00322     {\cf19 else}\par
00323     \{\par
00324         system(cellValue.c_str());\par
00325     \}\par
00326     {\cf20 // Allow the event to continue to be processed}\par
00327     {\cf17 event}.Skip();\par
00328 \}\par
