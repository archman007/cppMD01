00255 {\cf18 void} cppMD01Frame::PopulateMasterGrid()\par
00256 \{\par
00257     {\cf20 // Attempt to connect to the MySQL database}\par
00258     {\cf19 if} (!conn.connect({\cf22 "bcswebtools"}, {\cf22 "localhost"}, {\cf22 "bcs"}, {\cf22 "Peace007!amb"}))\par
00259     \{\par
00260         {\cf20 // Database connection failed, exit the function}\par
00261         {\cf20 // wxWrite("Database connection failed.");}\par
00262         {\cf19 return};\par
00263     \}\par
00264 \par
00265     {\cf20 // Execute the SQL query to fetch all records from the 'lk1' table, ordered by 'cat'}\par
00266     mysqlpp::Query query = conn.query({\cf22 "SELECT * FROM lk1 ORDER BY cat"});\par
00267     mysqlpp::StoreQueryResult res = query.store();\par
00268 \par
00269     {\cf20 // Clear the current contents of the grid and append rows based on the number of records fetched}\par
00270     masterGrid->ClearGrid();\par
00271     masterGrid->AppendRows(res.num_rows());\par
00272 \par
00273     {\cf20 // Populate the grid with data from the result set}\par
00274     {\cf19 for} ({\cf18 size_t} i = 0; i < res.num_rows(); ++i)\par
00275     \{\par
00276         masterGrid->SetCellValue(i, 0, res[i][0].c_str());  {\cf20 // Set cell value for column 0}\par
00277         masterGrid->SetCellValue(i, 1, res[i][1].c_str());  {\cf20 // Set cell value for column 1}\par
00278         masterGrid->SetCellValue(i, 2, res[i][2].c_str());  {\cf20 // Set cell value for column 2}\par
00279     \}\par
00280 \}\par
